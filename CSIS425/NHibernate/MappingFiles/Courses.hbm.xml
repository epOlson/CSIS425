<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
  namespace="CSIS425.Models"
  assembly="CSIS425.Models">
  
  <class name="CSIS425.Models.Model_Courses" table="Courses" lazy="false" >
    
      <id name="course_id" column="course_id" type="guid">
        <generator class="guid" />
      </id>
    
      <property name="holes">
        <column name="holes" sql-type="int" not-null="true" />
      </property>
    
      <property name="pars">
        <column name="pars" sql-type="varchar(50)" not-null="true" />
      </property>
    
  <property name="name">
        <column name="name" sql-type="varchar(50)" not-null="true" />
      </property>
  
  </class>
</hibernate-mapping>