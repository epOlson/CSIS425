<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
  namespace="CSIS425.Models"
  assembly="CSIS425.Models">

  <class name="CSIS425.Models.Model_Players" table="Players" lazy="false" >

    <composite-id name="key" >
      <key-property name="player_id"></key-property>
      <key-many-to-many name="round_id" class="Rounds" column="round_id"/>
    </composite-id>
    
    <property name="score">
      <column name="score" sql-type="varchar(50)" not-null="true" />
    </property>

  </class>
</hibernate-mapping>